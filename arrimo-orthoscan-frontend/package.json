{
    "name": "arrimo-orthoscan-frontend",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "main": "desktop/main.cjs",
    "productName": "Lab. Orthoscan",
    "build": {
        "appId": "com.orthoscan.lab",
        "productName": "Lab. Orthoscan",
        "directories": {
            "output": "desktop-dist"
        },
        "files": [
            "dist/**",
            "desktop/**",
            "package.json"
        ],
        "win": {
            "icon": "desktop/icon.ico",
            "target": [
                "nsis"
            ]
        },
        "nsis": {
            "oneClick": false,
            "createDesktopShortcut": true,
            "createStartMenuShortcut": true,
            "runAfterFinish": false
        }
    },
    "scripts": {
        "dev": "vite",
        "build": "tsc -b && vite build",
        "lint": "eslint .",
        "preview": "vite preview",
        "typecheck": "tsc -p tsconfig.app.json --noEmit",
        "test": "vitest",
        "test:ui": "vitest --ui",
        "test:e2e": "playwright test",
        "preflight:prod": "node ./scripts/preflightProd.mjs",
        "desktop:icon": "node ./desktop/make-icon.cjs",
        "desktop:dev": "node ./desktop/start-dev.cjs",
        "desktop:build": "set ELECTRON_BUILD=1&& npm run build && electron-builder --win",
        "qa:diagnostics": "node ./scripts/runDiagnosticsCli.mjs",
        "qa": "npm run qa:diagnostics && npm run test -- --run && npm run test:e2e && npm run build && node ./scripts/makeReport.mjs"
    },
    "dependencies": {
        "@supabase/supabase-js": "^2.95.3",
        "@vitejs/plugin-basic-ssl": "^2.1.4",
        "clsx": "^2.1.1",
        "lucide-react": "^0.563.0",
        "react": "^19.2.0",
        "react-dom": "^19.2.0",
        "react-router-dom": "^7.13.0",
        "tailwind-merge": "^3.4.0",
        "xlsx": "^0.18.5"
    },
    "devDependencies": {
        "@eslint/js": "^9.39.1",
        "@playwright/test": "^1.58.2",
        "@testing-library/jest-dom": "^6.9.1",
        "@testing-library/react": "^16.3.2",
        "@testing-library/user-event": "^14.6.1",
        "@types/node": "^24.10.1",
        "@types/react": "^19.2.5",
        "@types/react-dom": "^19.2.3",
        "@vitejs/plugin-react": "^5.1.1",
        "@vitest/ui": "^4.0.18",
        "autoprefixer": "^10.4.24",
        "electron": "^33.4.11",
        "electron-builder": "^24.13.3",
        "eslint": "^9.39.1",
        "eslint-plugin-react-hooks": "^7.0.1",
        "eslint-plugin-react-refresh": "^0.4.24",
        "globals": "^16.5.0",
        "jsdom": "^28.0.0",
        "playwright": "^1.58.2",
        "postcss": "^8.5.6",
        "tailwindcss": "^3.4.17",
        "to-ico": "^1.1.5",
        "typescript": "~5.9.3",
        "typescript-eslint": "^8.46.4",
        "vite": "^7.2.4",
        "vitest": "^4.0.18",
        "wait-on": "^7.2.0"
    }
}
